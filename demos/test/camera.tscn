script/source = "\nextends KinematicBody\n\n# member variables here, example:\n# var a=2\n# var b=\"textvar\"\n\nvar ray_node\n\nexport var view_sensitivity = 1.0\nexport var move_speed = 10\nexport var boost_modifier = 10\n\nfunc _ready():\n\t# Called every time the node is added to the scene.\n\t# Initialization here\n\tset_fixed_process(true)\n\tset_process_input(true)\n\tray_node = get_node(\"RayCast\")\n\tset_process(true)\n\t\n\n\nfunc _input(event):\n\tif event.type == InputEvent.MOUSE_MOTION:\n\t\trotate_x(event.relative_y * view_sensitivity * 0.05)\n\t\trotate_y(event.relative_x * view_sensitivity * 0.05)\n\nfunc _fixed_process(delta):\n\tvar transform = get_transform()\n\tvar move_vector = Vector3()\n\tif Input.is_action_pressed(\"move_up\"):\n\t\tmove_vector += Vector3(0, 0, -1)\n\tif Input.is_action_pressed(\"move_down\"):\n\t\tmove_vector += Vector3(0, 0, 1)\n\tif Input.is_action_pressed(\"move_left\"):\n\t\tmove_vector += Vector3(-1, 0, 0)\n\tif Input.is_action_pressed(\"move_right\"):\n\t\tmove_vector += Vector3(1, 0, 0)\n\tif Input.is_action_pressed(\"move_jump\"):\n\t\tmove_vector += Vector3(0, 1, 0)\n\tif Input.is_action_pressed(\"move_rotate_left\"):\n\t\trotate_z(0.03)\n\tif Input.is_action_pressed(\"move_rotate_right\"):\n\t\trotate_z(-0.03)\n\t\n\tif Input.is_action_pressed(\"ui_menu\"):\n\t\tInput.set_mouse_mode(Input.MOUSE_MODE_VISIBLE)\n\tif Input.is_action_pressed(\"fire\"):\n\t\tInput.set_mouse_mode(Input.MOUSE_MODE_CAPTURED)\n\t\tif ray_node.is_colliding():\n\t\t\tvar block_pos = ray_node.get_collision_point().floor()\n\t\t\tprint(block_pos)\n\t\t\tvar voxel_map = ray_node.get_collider().get_parent().get_parent()\n\t\t\tprint(voxel_map)\n\t\t\tvoxel_map.remove_block(block_pos)\n\t\t\t\n\t\n\tmove_vector = transform.translated(move_vector).origin - transform.origin\n\tmove_vector = move_vector.normalized() * move_speed * delta\n\tif Input.is_action_pressed(\"shift_modifier\"):\n\t\tmove_vector *= boost_modifier\n\tmove(move_vector)"

